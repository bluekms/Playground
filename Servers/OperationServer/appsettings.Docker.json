{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "File", "Args": { "path": "Logs/KmsOperation_.log", "rollingInterval": "Day" } }
    ]
  },
  "ConnectionStrings": {
    "AuthDb": "Server=172.18.0.2;Port=3306;Database=AuthDb;Uid=root;Pwd=Pineapple1;",
    "WorldDb": "Server=172.18.0.2;Port=3306;Database=WorldDb;Uid=root;Pwd=Pineapple1;",
    "RedisCache": "172.18.0.3:6379"
  },
  "StaticData": {
    "DataName": "StaticData",
    "Version": "latest",
    "ForceUpdate": "True",
    "ProviderType": "TarFile",
    "TarFileProvider": {
      "TarFilePath": "./StaticDataSrc"
    }
  }
}
