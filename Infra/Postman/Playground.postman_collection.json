									"    var response = pm.response.json();\r",
									"    pm.environment.set(\"SessionId\", response.sessionId);\r",
									"    if(response.worlds.length > 0)\r",
									"    {\r",
									"        pm.environment.set(\"WorldServerUrl\", response.worlds[0].address);\r",
									"        \r",
									"    }\r",
									"    console.log(\"SessionId: \" + pm.environment.get(\"SessionId\"));\r",
									"    console.log(\"WorldServerUrl: \" + pm.environment.get(\"WorldServerUrl\"));\r",
